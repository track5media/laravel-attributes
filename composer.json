{
    "name": "track5media/laravel-attributes",
    "description": "A clone of Rinvex Attributes; allows it to actually work with mariadb",
    "type": "library",
    "keywords": [
        "eav",
        "eloquent",
        "laravel",
        "dynamic",
        "robust",
        "schema",
        "model",
        "value",
        "entity",
        "database",
        "attribute",
        "integrated",
        "intelligent",
        "entity-attribute-value"
    ],
    "license": "MIT",
    "homepage": "https://track5media.com",
    "support": {
        "email": "help@rinvex.com",
        "issues": "https://github.com/track5media/laravel-attributes/issues",
        "source": "https://github.com/track5media/laravel-attributes",
        "docs": "https://github.com/track5media/laravel-attributes/blob/master/README.md"
    },
    "authors": [
        {
            "name": "Dan Mattern",
            "homepage": "track5media.com",
            "email": "dan@track5media.com"
        },
        {
            "name": "Rinvex LLC",
            "homepage": "https://rinvex.com",
            "email": "help@rinvex.com"
        },
        {
            "name": "Abdelrahman Omran",
            "homepage": "https://omranic.com",
            "email": "me@omranic.com",
            "role": "Project Lead"
        },
        {
            "name": "Israel Ortu√±o",
            "email": "israel@devio.es"
        },
        {
            "name": "The Generous Laravel Community",
            "homepage": "https://github.com/rinvex/laravel-attributes/contributors"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "illuminate/console": "~5.7.0",
        "illuminate/database": "~5.7.0",
        "illuminate/support": "~5.7.0",
        "jeremeamia/superclosure": "^2.3.0",
        "rinvex/laravel-cacheable": "^1.0.0",
        "rinvex/laravel-support": "v1.0.0",
        "spatie/eloquent-sortable": "^3.4.0",
        "spatie/laravel-sluggable": "^2.1.0",
        "spatie/laravel-translatable": "^2.1.0",
        "watson/validating": "^3.1.0"
    },
    "require-dev": {
        "codedungeon/phpunit-result-printer": "^0.22.0",
        "illuminate/container": "~5.7.0",
        "orchestra/testbench": "^3.6",
        "phpunit/phpunit": "^7.0.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "Rinvex\\Attributes\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rinvex\\Attributes\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rinvex\\Attributes\\Providers\\AttributesServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
